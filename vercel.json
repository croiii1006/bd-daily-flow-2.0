{
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/[...path].js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/notify/daily?title=提醒预览&url=https%3A%2F%2Fbd-daily-flow-2-0.vercel.app%2F",
      "schedule": "0 2 * * 1-5"
    },
    {
      "path": "/api/notify/daily?title=每日表单&url=https%3A%2F%2Fbd-daily-flow-2-0.vercel.app%2F",
      "schedule": "40 8 * * 1-5"
    }
  ]
}
